{
  "name": "server",
  "version": "1.0.0",
  "description": "calcio balilla per molo17",
  "main": "index.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "build": "npx tsc",
    "lint": "eslint ./App",
    "install:frontend": "cd ./App/Presentation && npm i",
    "start:frontend": "cd ./App/Presentation && npm run start",
    "test:frontend": "cd ./App/Presentation && npm run test",
    "start:backend": "cd ./App && nodemon index.ts",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\""
  },
  "proxy": "http://localhost:3001",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "mysql2": "^3.11.5",
    "nano": "^10.1.4",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.17.0",
    "@jest/globals": "^29.7.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.1.0",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^2.5.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ]
    }
  }
}
